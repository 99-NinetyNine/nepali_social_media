# Copy this file to .env and update the values

# API Configuration
REACT_APP_API_URL=http://localhost:8000/api

# App Configuration  
REACT_APP_NAME=Social Media Platform
REACT_APP_VERSION=1.0.0

# Feature Flags
REACT_APP_ENABLE_GOOGLE_AUTH=true
REACT_APP_ENABLE_SUBSCRIPTIONS=true
REACT_APP_ENABLE_ADS=true

# Environment
NODE_ENV=development

# Optional: WebSocket URL for real-time features (if needed in future)
# REACT_APP_WS_URL=ws://localhost:8000/ws

# Optional: CDN or static files URL (if needed)
# REACT_APP_STATIC_URL=http://localhost:8000/static